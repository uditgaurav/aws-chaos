{
    "Version": "2012-10-17",
    "Statement": [
        {
        "Effect": "Allow",
        "Action": [
          "lambda:GetFunctionConfiguration",
          "lambda:GetFunction",
          "lambda:UpdateFunctionConfiguration"
        ],
        "Resource": "*"
      },
      {
        "Effect": "Allow",
        "Action": [
          "lambda:ListLayers"
        ],
        "Resource": "*"
      },
      {
        "Effect": "Allow",
        "Action": [
          "lambda:GetLayerVersion",
          "lambda:ListVersionsByFunction"
        ],
        "Resource": "*"
      },
      {
        "Effect": "Allow",
        "Action": [
          "cloudwatch:PutMetricData"
        ],
        "Resource": "*"
      },
      {
        "Effect": "Allow",
        "Action": [
          "aws:logs:*"
        ],
        "Resource": "*"
      }
    ]
  }
